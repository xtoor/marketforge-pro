{
  "name": "marketforge-pro",
  "version": "1.0.0",
  "description": "Advanced financial visualization platform with TradingView integration, ML strategies, and multi-broker support",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "electron .",
    "build": "tsc && vite build",
    "build:electron": "npm run build && electron-builder --win --x64",
    "build:electron:portable": "npm run build && electron-builder --win portable",
    "build:all": "npm run build && electron-builder -mwl",
    "test": "jest",
    "test:backend": "cd backend && pytest",
    "lint": "eslint . --ext ts,tsx",
    "install:all": "npm install && cd backend && pip install -r requirements.txt",
    "setup:tradingview": "git submodule add https://github.com/tradingview/lightweight-charts.git frontend/src/tradingview/lightweight-charts",
    "setup:resonance": "git submodule add https://github.com/resonance-ai/scanner-v13.git backend/resonance",
    "start:backend": "cd backend && uvicorn api.main:app --reload --port 8000",
    "start:frontend": "vite --port 3000",
    "package:win": "npm run build && electron-builder --win --publish never",
    "release": "npm run build && electron-builder --publish always"
  },
  "keywords": [
    "trading",
    "visualization",
    "tradingview",
    "cryptocurrency",
    "machine-learning"
  ],
  "author": "MarketForge-Pro Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "electron-store": "^8.1.0",
    "lightweight-charts": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
